<refentry id="element.note">
  <refnamediv>
    <refname>note</refname>
    <refpurpose>Additional information</refpurpose>
  </refnamediv>

  <refsynopsisdiv><title>Synopsis</title>
    <refsect2><title>Content Model</title>
      <synopsis><$CONTENT_MODEL$></synopsis>
    </refsect2>
    <refsect2><title>Attributes</title>
      <$ATTRIBUTES_TABLE$>
    </refsect2>
  </refsynopsisdiv>

  <refsect1><title>Description</title>
    <para>
      The <sgmltag class="element">note</sgmltag> element contains additional
      information about its parent element.
    </para>

    <refsect2><title>Parents</title>
      <para>
        <sgmltag class="element">note</sgmltag> may be contained in these
        elements:
        <$PARENTS$>
      </para>
    </refsect2>

    <!-- Use this section to describe element attributes, if any
    <refsect2><title>Attributes</title>
