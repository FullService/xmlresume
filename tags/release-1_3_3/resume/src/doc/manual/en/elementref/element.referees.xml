  <refnamediv>
    <refname>referees</refname>
    <refpurpose>Contains one or more referees</refpurpose>
  </refnamediv>

  <refsynopsisdiv><title>Synopsis</title>
    <refsect2><title>Content Model</title>
      <synopsis><$CONTENT_MODEL$></synopsis>
    </refsect2>
