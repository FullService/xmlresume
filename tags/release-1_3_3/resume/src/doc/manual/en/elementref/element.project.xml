<refentry id="element.project">
  <refnamediv>
    <refname>project</refname>
    <refpurpose>Container for information about a project</refpurpose>
  </refnamediv>

  <refsynopsisdiv><title>Synopsis</title>
    <refsect2><title>Content Model</title>
      <synopsis><$CONTENT_MODEL$></synopsis>
    </refsect2>
