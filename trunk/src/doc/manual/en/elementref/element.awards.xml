<refentry id="element.awards">
  <refnamediv>
    <refname>awards</refname>
    <refpurpose>Container for one or more awards</refpurpose>
  </refnamediv>

  <refsynopsisdiv><title>Synopsis</title>
    <refsect2><title>Content Model</title>
      <synopsis><?resumevar CONTENT_MODEL?></synopsis>
    </refsect2>
    <refsect2><title>Attributes</title>
      <?resumevar ATTRIBUTES_TABLE?>
    </refsect2>
  </refsynopsisdiv>

  <refsect1><title>Description</title>
    <para>
      The <sgmltag class="element">awards</sgmltag> element contains one or more
      <link linkend="element.award"><sgmltag
      class="element">award</sgmltag></link>s.
    </para>

    <refsect2><title>Parents</title>
      <para>
        <sgmltag class="element">awards</sgmltag> may be contained in these
        elements:
        <?resumevar PARENTS?>
      </para>
    </refsect2>

