<refentry id="element.organization">
  <refnamediv>
    <refname>organization</refname>
    <refpurpose>A name of an organization</refpurpose>
  </refnamediv>

  <refsynopsisdiv><title>Synopsis</title>
    <refsect2><title>Content Model</title>
      <synopsis><?resumevar CONTENT_MODEL?></synopsis>
    </refsect2>
    <refsect2><title>Attributes</title>
      <?resumevar ATTRIBUTES_TABLE?>
    </refsect2>
  </refsynopsisdiv>

  <refsect1><title>Description</title>
    <para>
      The <sgmltag class="element">organization</sgmltag> element contains the
      name of a profession or other organization.
    </para>

    <refsect2><title>Parents</title>
      <para>
        <sgmltag class="element">organization</sgmltag> may be contained in these
        elements:
        <?resumevar PARENTS?>
      </para>
    </refsect2>

    <refsect2><title>Attributes</title>
      <variablelist>

        <varlistentry>
          <term><sgmltag class="attribute">id</sgmltag></term>
          <listitem>
            <para>
              A string used to uniquely identify an organization. The
              organization may then be referred to by this identifier.
            </para>
          </listitem>
        </varlistentry>

      </variablelist>
    </refsect2>

  </refsect1>

  <refsect1><title>See Also</title>
    <para>
      <simplelist type="inline">
        <member><link linkend="element.institution"><sgmltag class="element">institution</sgmltag></link></member>
      </simplelist>.
    </para>
  </refsect1>
  -->

  <refsect1><title>Examples</title>

    <informalexample><programlisting><![CDATA[
<membership>
  <organization>Croquet Players' Association of the UK</organization>
</membership>
    ]]></programlisting></informalexample>

  </refsect1>

</refentry>
